        android:supportsRtl="true"
        android:theme="@style/Theme.ProdecoConf">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyDeoei9bNfDtOcZntCCzz-TMjkW2XtoOzg"/>

        <activity android:name="com.prodeco.conf.view.ui.activities.SplashscreenActivity">
